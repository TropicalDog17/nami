{
  "name": "nami-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:ts": "eslint src --ext .ts,.tsx --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "./node_modules/.bin/playwright test",
    "test:e2e:headed": "./node_modules/.bin/playwright test --headed",
    "test:e2e:debug": "./node_modules/.bin/playwright test --debug",
    "test:e2e:report": "./node_modules/.bin/playwright show-report",
    "test:e2e:clean": "rm -rf test-results playwright-report",
    "test:e2e:install": "playwright install",
    "test:e2e:update": "npx playwright update",
    "test:e2e:isolated": "PORT=3001 VITE_API_BASE_URL=http://localhost:8001 ./node_modules/.bin/playwright test --config=playwright-isolated.config.js",
    "test:e2e:isolated:headed": "PORT=3001 VITE_API_BASE_URL=http://localhost:8001 ./node_modules/.bin/playwright test --config=playwright-isolated.config.js --headed",
    "test:setup": "cd .. && ./scripts/setup-test-env.sh",
    "test:backend": "cd ../backend && ./scripts/start-test-backend.sh",
    "test:frontend": "PORT=3001 VITE_API_BASE_URL=http://localhost:8001 npm run dev"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "chart.js": "^4.5.0",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "cypress": "^14.5.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^4.4.5",
    "vitest": "^4.0.14"
  }
}
