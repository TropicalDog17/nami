{
  "name": "nami-ai-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "fmt": "prettier --write .",
    "check": "tsc --noEmit",
    "test": "npm run build && vitest run",
    "test:unit": "jest",
    "test:llm": "jest tests/llm.integration.test.ts",
    "test:llm:watch": "jest tests/llm.integration.test.ts --watch",
    "test:llm:coverage": "jest tests/llm.integration.test.ts --coverage",
    "test:llm:simple": "node test-llm-simple.mjs",
    "test:llm:real": "node test-real-llm.mjs",
    "process-statement": "tsx src/cli/processBankStatement.ts",
    "process-statement:dry": "tsx src/cli/processBankStatement.ts --dry-run",
    "process-statement:fast": "tsx src/cli/processBankStatement.ts --skip-ai"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@toon-format/toon": "^0.7.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-prom-bundle": "^7.0.0",
    "multer": "^2.0.2",
    "openai": "^4.57.0",
    "p-limit": "^7.2.0",
    "pino": "^9.3.2",
    "prom-client": "^15.1.0",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.7.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
