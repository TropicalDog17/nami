{
    "name": "nami-portfolio-backend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "ts-node-dev --transpile-only --respawn src/index.ts",
        "build": "tsc -p .",
        "start": "node dist/index.js",
        "fmt": "prettier --write .",
        "test": "vitest run",
        "test:watch": "vitest",
        "purge:transactions": "ts-node-dev --transpile-only --exit-child src/scripts/purgeTransactions.ts",
        "migrate:backfill-spending-vault": "ts-node-dev --transpile-only --exit-child src/scripts/backfillSpendingVault.ts",
        "migrate:income-to-spend": "ts-node-dev --transpile-only --exit-child src/scripts/migrateIncomeToSpend.ts",
        "migrate:to-database": "ts-node-dev --transpile-only --exit-child src/scripts/migrate-to-database.ts",
        "migrate:rollback-database": "ts-node-dev --transpile-only --exit-child src/scripts/rollback-database.ts",
        "migrate:to-prod": "ts-node-dev --transpile-only --exit-child src/scripts/migrate-to-prod.ts"
    },
    "dependencies": {
        "@asteasolutions/zod-to-openapi": "^7.3.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@vercel/node": "^5.5.16",
        "axios": "^1.7.7",
        "better-sqlite3": "^12.5.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.19.2",
        "express-prom-bundle": "^7.0.0",
        "p-limit": "^3.0.0",
        "pino": "^9.3.2",
        "prom-client": "^15.1.0",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^9.0.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^22.7.4",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "pino-pretty": "^11.2.2",
        "prettier": "^3.7.4",
        "supertest": "^7.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.6.3",
        "vitest": "^2.1.3"
    }
}
